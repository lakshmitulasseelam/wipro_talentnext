package jfun;

public class SearchingArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Enter a value:");
		int n[] = new int[5] ;
		int x = 0;
		int index = -1;
		
		for(int i = 0; i< n.length;i++) {
			if(n[i] == x) {
				index = i;
				break;
			}
		};
        
		System.out.println(index);

	}

}
